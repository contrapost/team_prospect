<div class="container">
  <div class="row">
    <% @group_members.order(:id).each do |group_member| %>
        <div class="col-md-4">
          <%= link_to group_member do %>
          <%= image_tag group_member.photo.url (:thumb) %>
              <% end %>
          <h2><%= group_member.first_name %> <%= group_member.last_name %></h2>
        </div>

    <% end %>
  </div>
</div>

